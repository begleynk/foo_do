<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <h2 class='page-header'>Current Projects</h2>
    <table class='table'>
      <thead>
        <tr>
          <th>Name</th>
          <th>Due</th>
          <th>Actions</th>
        </tr>
      </thead>
      <tbody>
        <% @projects.each do |p| %>
          <%= render "project_row", project: p %>
        <% end %>
      </tbody>
    </table>
  </div>
</div>